{
  "name": "@chialab/loock",
  "version": "2.1.0",
  "description": "An accessibility helper for keyboard navigation on the web.",
  "structure": "module",
  "types": "types/index.d.ts",
  "lib": "src/index.js",
  "main": "dist/cjs/loock.js",
  "module": "dist/esm/loock.js",
  "browser": "dist/umd/loock.js",
  "exports": {
    "require": "./dist/cjs/loock.js",
    "default": "./dist/esm/loock.js"
  },
  "license": "MIT",
  "author": "Chialab",
  "repository": {
    "type": "git",
    "url": "git@github.com:chialab/loock.git"
  },
  "homepage": "https://www.chialab.io/p/loock",
  "bugs": {
    "url": "https://github.com/chialab/loock/issues",
    "email": "dev@chialab.io"
  },
  "devDependencies": {
    "@chialab/eslint-config": "^2.3.0",
    "eslint": "^7.25.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@chialab/proteins": "^3.0.0"
  },
  "directories": {
    "src": "src",
    "lib": "dist",
    "test": "test"
  },
  "scripts": {
    "build": "yarn types && rimraf dist && rna build --production",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --declarationDir 'types'",
    "watch": "rna build --watch",
    "test": "rna unit 'test/*.tests.js' --saucelabs --coverage --concurrency 1",
    "lint": "eslint src",
    "start": "yarn install + rna build --watch",
    "prepack": "yarn build"
  }
}
