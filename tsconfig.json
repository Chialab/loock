
{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "outDir": "dist",
    "declaration": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "include": [
    "src"
  ],
  "exclude": []
}
